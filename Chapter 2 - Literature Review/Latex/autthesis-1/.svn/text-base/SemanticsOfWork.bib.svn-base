@article{SumnerRyan1994,
   Author = {Mary Sumner and Terence Ryan},
   Title = {The Impact of \textsc{case}: Can It Achieve critical success factors?},
   Journal = {Journal of Systems Management},
   Volume = {45},
   Number = {6},
   Pages = {16--21},
   Abstract = {Computer-assisted software engineering (CASE) is viewed as a strategy to reduce development time, to cut maintenance costs, and to improve the discipline of information systems development. Yet, even with its many benefits, most organizations have found it difficult to implement CASE. Some of the obstacles to introducing CASE are cost, resistance by systems developers, and unacceptable learning curves. Two questions are examined: 1. What are the critical success factors in information systems analysis and design? 2. Do CASE tools support these critical success factors? The findings illustrate a discontinuity between the use of CASE and the achievement of critical success factors in systems development. It is found that for CASE to be effective, an organization may need to view information systems development as a work system.},
      Year = {1994} }
      
      
@article{Sundin2005,
	Abstract = {Purpose -- To show that the neo-pragmatist position of Richard Rorty, when combined with a sociocultural perspective, provides library and information science (LIS) with a forceful epistemological tool. \\
Design/methodology/approach -- Literature-based conceptual analysis of: historical development of pragmatism in relation to other epistemological positions; neo-pragmatism as a non-dualist, both purpose and communication oriented, epistemology; and a sociocultural perspective within pedagogy, originated from the Russian researcher Lev Vygotsky. \\
Findings -- Brought together, a neo-pragmatist, sociocultural perspective contributes to a focus on people's actions through the use of linguistic and physical tools. As a tangible example of how neo-pragmatism can be applied as an epistemological tool within LIS, information seeking seen as communicative participation is discussed. This article unites a perspective on information seeking as communicative participation with the neo-pragmatist concepts of ``tools'' and ``communities of justification''. The article is concluded by an assessment of neo-pragmatism as an epistemological position within LIS, including those research issues that arise from this position and that are introduced along the way. \\
Practical implications -- In its focus on usability, the neo-pragmatist position provides a possible bridge between academic and other professional practices in the field of LIS. \\
Originality/value -- Provides, through the means of neo-pragmatism, an argument for the necessity of epistemological argumentation within LIS.},
	Author = {Olof Sundin and Jenny Johannisson},
	Date-Added = {2006-10-19 11:34:41 +1300},
	Date-Modified = {2006-10-19 11:37:02 +1300},
	Journal = {Journal of Documentation},
	Keywords = {Pragmatism, information science},
	Number = {1},
	Pages = {23--43},
	Title = {Pragmatism, neo-pragmatism and sociocultural theory: Communicative participation as a perspective in {LIS}},
	Volume = {61},
	Year = {2005}}


