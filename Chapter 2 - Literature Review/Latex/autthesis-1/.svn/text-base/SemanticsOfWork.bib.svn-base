@article{Benois2002,
	Abstract = {The field of library and information science (US) has built its practice and research perspective on a logico-analytic philosophy, which has generated useful, tangible technologies. Lately there is interest in nonempirical philosophies, such as hermeneutics and critical theory, to fulfill better US' service oriented mission of providing ``information'' to user populations. One dimension of critical theory, Jurgen Habermas' theory of communicative action, can expand US' conception of information and supplement logico-empirical research methods and research practices. This article examines how US, particularly research into librarian-patron interaction and information system design, favors an empiricist view of language and, as a consequence, may be limiting its effectiveness. It proposes how the field might expand to include communicative action in fulfilling its mission and research agenda.},
	Author = {Gerald Benois},
	Date-Added = {2006-10-19 10:58:55 +1300},
	Date-Modified = {2006-10-19 11:02:50 +1300},
	Journal = {Library Quarterly},
	Keywords = {philosophy of information, philosophy of computer science, information science, critical theory},
	Number = {4},
	Pages = {441--471},
	Title = {Towards a critical theoretic perspective in information systems},
	Volume = {72},
	Year = {2002}}
	
