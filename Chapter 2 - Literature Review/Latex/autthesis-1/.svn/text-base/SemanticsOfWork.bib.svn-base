@book{White2002,
	Abstract = {In this book, one of America's leading philosophers offers a sweeping reconsideration of the philosophy of culture in the twentieth century. Morton White argues that the discipline in much more important than is often recognized, and that his version of holistic pragmatism can accommodate its breadth. Going beyond Quine's dictum that philosophy of science is philosophy enough, White suggests that it should contain the word ``culture'' in place of ``science''. He defends the holistic view that scientific belief is tested by experience but that such testing is rightly applied to systems or conjunctions of beliefs, not isolated beliefs. He adds, however, that we test ethical systems by appealing to feelings of moral obligation as well as to sensory experiences.},
	Address = {41 William Street, Princeton, New Jersey, USA},
	Author = {Morton Gabriel White},
	Isbn = {0-691-09656-2},
	Keywords = {Pragmatism, Culture, Philosophy, Holism},
	Publisher = {Princeton University Press},
	Title = {A philosophy of culture: The scope of holistic pragmatism},
	Year = {2002}}
	
@article{White2006a,
	institution = {The Institution of Engineering and Technology},
	title = {Web 2.0 and enterprise business intelligence},
	journal = {Cutter IT Journal},
	author = {White, L.},
	volume = {19},
	number = {10},
	year = {2006},
	month = {October},
	pages = {19--23},
	abstract = {Web 2.0 has received a lot of press lately. So far, the primary use has been in creating new applications for users of the public Internet. In this article, the author discusses a potential alternative use for enterprise users. Specifically, applying these techniques to business intelligence (BI) solutions creates applications that are better adopted by users and also allows users to increase the "intelligence" created by the applications. The subject of this article is not to introduce the reader to Web 2.0 or BI explicitly but rather to explain how the convergence of these ideas may be useful. Therefore, the author focuses on which aspects of BI and Web 2.0 are relevant, how to incorporate them into new systems, and what is minimally required to support these systems in the enterprise},
	keywords = {competitive intelligence;Internet;Web services;Web 2.0;enterprise business intelligence;Internet;BI;Web services;}} 

