@article{Virtue2009,
	title = {Finding the Sweet Spot: {B}alancing the Risks and Rewards of Social Networking Technologies in the Enterprise},
	journal = {Cutter IT Journal},
	author = {Virtue, T.},
	volume = { 22},
	number = { 10},
	year = {2009},
	month = {Oct},
	pages = {6 - 9},	
	issn = {1048-5600},
	address = {USA},
	abstract = {Although the foundation of modern technology is based on 1s and 0s, or ``on'' or ``off,'' many of the technology deployments in today's enterprise are not that simple. Particularly from a business risk perspective, it is more complicated than black or white, and we must learn to manage a lot more gray. The Chinese philosophical concept of yin-yang is based on the belief that there are two complementary forces in the universe and one cannot be viewed as better than the other. Instead, they are both necessary, and a balance of both is what we should strive for. In regard to Web 2.0 and social networking technologies in the enterprise, this is exactly the paradigm in which it needs to start operating. The organization would likely fall behind its competitors in terms of achieving innovation, meeting customer demands, and taking advantage of efficiencies and new revenue streams. On the other extreme, consider the organization that deploys Web 2.0 and social networking technologies throughout the enterprise with few or no controls, policies, or guidance in place. The organization could well fall victim to an increased number of security attacks, see a decline in employee productivity, and/or find itself facing a barrage of HR and legal problems. The paper mentions that it needs to embrace Web 2.0 and social networking technologies (and the associated benefits) and develop innovative technology and policy solutions in order to reduce risk to a manageable level.},
	keywords = {Internet;organisational aspects;risk management;social networking (online); business risk perspective;yin-yang concept;Web 2.0;social networking technology;organization;innovative technology;} } 


@article{Visala1992,
	Abstract = {The paper considers the ethical issues of technology in general and the special problems of information systems. It takes us from the position of technology and science in western society as an idealized calling of the human being and as the principal forces for progress, via technology as ideology, to the ethical responsibility of an individual planner and the social dimensions of information systems. A number of basic ethical dilemmas are encountered and analyzed. The tasks for information systems research are provided with a legitimation from the ethical point of view.},
	Author = {Seppo Visala},
	Journal = {Communications of the {ACM}},
	Keywords = {ethics, Marx, dialectical materialism},
	Pages = {210--220},
	Publisher = {ACM},
	Title = {Ethical dilemmas in information systems research and development},
	Year = {1992}}

